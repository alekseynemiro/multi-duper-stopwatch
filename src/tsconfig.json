{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@components": ["./Components"],
      "@components/*": ["./Components/*"],
      "@config": ["./Config/index"],
      "@config/*": ["./Config/*"],
      "@data": ["./Data"],
      "@data/*": ["./Data/*"],
      "@dto/*": ["./Core/Dto/*"],
      "@pages/*": ["./Pages/*"],
      "@styles": ["./Styles"],
      "@styles/*": ["./Styles/*"],
      "@utils/*": ["./Utils/*"]
    }
  }
}
